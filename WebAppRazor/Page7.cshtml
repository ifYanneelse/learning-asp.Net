@{
    Layout = null;

    var head = "Módulo 5";
    var titulo = "ASP.NET Razor";

    var setor = "Hortifruti delivery";
    string[] itens = { "abacate", "abacaxi", "abóbora", "alface", "abobrinha", "pêssego" };
    float[] preco = { 3.38F, 7.55F, 1.85F, 5.25F, 4.12F, 2.56F };
    int[] quantidade = {5, 2, 10, 32, 12, 4 };
    var status = "";

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>>@head</title>
    <link href="~/Styles.css" rel="stylesheet" />
</head>
<body>
    <div>
        <h1>@titulo</h1>

        <h2>@setor</h2>

        <table>
            <tr>
                <td class="headtable">Item</td>
                <td class="headtable">Preço</td>
                <td class="headtable">Qtde</td>
                <td class="headtable">Status</td>
            </tr>

            <tr>
                @for (int i = 0; i < itens.Length; i++)
                {

                    if (quantidade[i] >= 5)
                    { status = " ";}
                    else
                    { status = "acabando";}

                <tr>
                    <td>
                        @(itens[i])
                    </td>
                    <td>
                        @(preco[i])
                    </td>
                    <td>
                        @(quantidade[i])
                    </td>
                    <td class="@status">
                        @(status)
                    </td>
                </tr>

                }
            </tr>
        </table>

        </div>
</body>
</html>
